{
  "name": "evorto",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:evorto": "node dist/evorto/server/server.mjs",
    "serve:instrumented:evorto": "node --import ./instrument.mjs dist/evorto/server/server.mjs",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write .",
    "update": "ng update @angular/cli @angular/cdk @angular/core @angular/material angular-eslint --allow-dirty",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui --workers 2",
    "e2e:browser": "playwright test --workers 1 --headed",
    "e2e:report": "playwright show-report",
    "e2e:docs": "playwright test --project docs",
    "e2e:generate-states": "playwright test --project=setup",
    "stripe:listen": "stripe listen --forward-to http://localhost:4200/webhooks/stripe",
    "generate:colors": "ng generate @angular/material:theme-color --include-high-contrast --primary-color=#0891b2 --interactive=false --directory=src/ --force",
    "generate:colors:esn": "ng generate @angular/material:theme-color --include-high-contrast --primary-color=#00aeef --secondary-color=#ec008c --tertiary-color=#7ac143 --interactive=false --directory=src/_esn --force",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org lukas-heddendorp --project evorto ./dist && sentry-cli sourcemaps upload --org lukas-heddendorp --project evorto ./dist",
    "push:database": "drizzle-kit push --force",
    "setup:database": "tsx helpers/database.ts",
    "reset:database": "run push:database && run setup:database",
    "docker:stop": "docker compose down",
    "docker:start": "yarn docker:stop && docker compose up --build -d",
    "docker:start-test": "yarn docker:stop && docker compose up --build",
    "migrate": "yarn tsx migration/index.ts",
    "check:tiptap-license": "node helpers/check-tiptap-license.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.3",
    "@angular/cdk": "21.1.3",
    "@angular/common": "^21.1.3",
    "@angular/compiler": "^21.1.3",
    "@angular/core": "^21.1.3",
    "@angular/forms": "^21.1.3",
    "@angular/material": "21.1.3",
    "@angular/material-luxon-adapter": "^21.1.3",
    "@angular/platform-browser": "^21.1.3",
    "@angular/platform-browser-dynamic": "^21.1.3",
    "@angular/platform-server": "^21.1.3",
    "@angular/router": "^21.1.3",
    "@angular/ssr": "^21.1.2",
    "@fortawesome/angular-fontawesome": "4.0.0",
    "@fortawesome/duotone-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@googlemaps/js-api-loader": "^2.0.2",
    "@heddendorp/tanstack-angular-query": "0.2.2",
    "@heddendorp/trpc-link-angular": "^0.0.5",
    "@material/material-color-utilities": "patch:@material/material-color-utilities@npm%3A0.4.0#~/.yarn/patches/@material-material-color-utilities-npm-0.4.0-9d48ca70b8.patch",
    "@neondatabase/serverless": "^1.0.2",
    "@ng-web-apis/common": "^5.1.0",
    "@paralleldrive/cuid2": "^3.3.0",
    "@sentry/angular": "^10.38.0",
    "@sentry/cli": "^3.1.0",
    "@sentry/node": "^10.38.0",
