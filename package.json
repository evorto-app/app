{
  "name": "evorto",
  "version": "0.0.0",
  "scripts": {
    "ng": "NG_BUILD_PARTIAL_SSR=1 bunx --bun ng",
    "start": "NG_BUILD_PARTIAL_SSR=1 bunx --bun ng serve",
    "build": "NG_BUILD_PARTIAL_SSR=1 bunx --bun ng build",
    "watch": "NG_BUILD_PARTIAL_SSR=1 bunx --bun ng build --watch --configuration development",
    "test": "bunx --bun ng test",
    "serve:ssr:evorto": "bun --bun dist/evorto/server/server.mjs",
    "serve:instrumented:evorto": "bun --bun --import ./instrument.mjs dist/evorto/server/server.mjs",
    "lint": "bunx --bun ng lint",
    "lint:fix": "bunx --bun ng lint --fix",
    "format": "prettier --write .",
    "update": "bunx --bun ng update @angular/cli @angular/cdk @angular/core @angular/material angular-eslint --allow-dirty",
    "e2e": "bash -lc 'eval \"$(bun helpers/testing/runtime-env.mjs)\" && bunx --bun playwright test'",
    "e2e:ui": "bash -lc 'eval \"$(bun helpers/testing/runtime-env.mjs)\" && bunx --bun playwright test --ui --workers 2'",
    "e2e:browser": "bash -lc 'eval \"$(bun helpers/testing/runtime-env.mjs)\" && bunx --bun playwright test --workers 1 --headed'",
    "e2e:report": "bunx --bun playwright show-report",
    "e2e:docs": "bash -lc 'eval \"$(bun helpers/testing/runtime-env.mjs)\" && bunx --bun playwright test --project docs'",
    "e2e:generate-states": "bash -lc 'eval \"$(bun helpers/testing/runtime-env.mjs)\" && bunx --bun playwright test --project=setup'",
    "stripe:listen": "stripe listen --forward-to http://localhost:4200/webhooks/stripe",
    "generate:colors": "bunx --bun ng generate @angular/material:theme-color --include-high-contrast --primary-color=#0891b2 --interactive=false --directory=src/ --force",
    "generate:colors:esn": "bunx --bun ng generate @angular/material:theme-color --include-high-contrast --primary-color=#00aeef --secondary-color=#ec008c --tertiary-color=#7ac143 --interactive=false --directory=src/_esn --force",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org lukas-heddendorp --project evorto ./dist && sentry-cli sourcemaps upload --org lukas-heddendorp --project evorto ./dist",
    "push:database": "drizzle-kit push --force",
    "setup:database": "bunx --bun tsx helpers/database.ts",
    "reset:database": "bun run push:database && bun run setup:database",
    "docker:stop": "bash -lc 'eval \"$(bun helpers/testing/runtime-env.mjs)\" && docker compose down'",
    "docker:start": "bash -lc 'eval \"$(bun helpers/testing/runtime-env.mjs)\" && docker compose down && docker compose up --build -d'",
    "docker:start-test": "bash -lc 'eval \"$(bun helpers/testing/runtime-env.mjs)\" && docker compose down && docker compose up --build'",
    "migrate": "bunx --bun tsx migration/index.ts",
    "check:tiptap-license": "bun helpers/check-tiptap-license.mjs",
    "cleanup:testing:receipt-images": "bunx --bun tsx helpers/cleanup-testing-receipt-images.ts",
    "cleanup:testing:receipt-images:dry-run": "bunx --bun tsx helpers/cleanup-testing-receipt-images.ts --dry-run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.3",
    "@angular/cdk": "21.1.3",
    "@angular/common": "^21.1.3",
    "@angular/compiler": "^21.1.3",
    "@angular/core": "^21.1.3",
    "@angular/forms": "^21.1.3",
    "@angular/material": "21.1.3",
    "@angular/material-luxon-adapter": "^21.1.3",
    "@angular/platform-browser": "^21.1.3",
    "@angular/platform-browser-dynamic": "^21.1.3",
    "@angular/platform-server": "^21.1.3",
    "@angular/router": "^21.1.3",
    "@angular/ssr": "^21.1.2",
    "@aws-sdk/client-s3": "^3.984.0",
    "@aws-sdk/s3-request-presigner": "^3.984.0",
    "@effect/platform": "^0.94.3",
    "@effect/rpc": "^0.73.0",
    "@fortawesome/angular-fontawesome": "4.0.0",
    "@fortawesome/duotone-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@googlemaps/js-api-loader": "^2.0.2",
    "@heddendorp/effect-angular-query": "^0.0.5",
    "@heddendorp/tanstack-angular-query": "0.2.2",
    "@heddendorp/trpc-link-angular": "^0.0.5",
    "@material/material-color-utilities": "patch:@material/material-color-utilities@npm%3A0.4.0#./patches/@material-material-color-utilities-npm-0.4.0-9d48ca70b8.patch",
    "@neondatabase/serverless": "^1.0.2",
    "@ng-web-apis/common": "^5.1.0",
    "@paralleldrive/cuid2": "^3.3.0",
    "@sentry/angular": "^10.38.0",
    "@sentry/cli": "^3.1.0",
    "@sentry/node": "^10.38.0",
    "@sentry/profiling-node": "^10.38.0",
    "@tailwindcss/typography": "0.5.19",
    "@tanstack/angular-query-experimental": "5.90.25",
    "@tiptap/core": "^3.19.0",
    "@tiptap/extension-file-handler": "^3.19.0",
    "@tiptap/extension-image": "^3.19.0",
    "@tiptap/extension-link": "^3.19.0",
    "@tiptap/extension-table": "^3.19.0",
    "@tiptap/extension-text-style": "^3.19.0",
    "@tiptap/pm": "^3.19.0",
    "@tiptap/starter-kit": "^3.19.0",
    "@trpc/client": "^11.9.0",
    "@trpc/server": "^11.9.0",
    "auth0": "^5.3.0",
    "cloudflare": "^5.2.0",
    "compression": "^1.8.1",
    "consola": "^3.4.2",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "drizzle-orm": "1.0.0-beta.14-a36c63d",
    "effect": "^3.19.16",
    "es-toolkit": "^1.44.0",
    "express": "^5.2.1",
    "express-openid-connect": "^2.19.4",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "html-to-text": "^9.0.5",
    "inter-ui": "^4.1.1",
    "luxon": "^3.7.2",
    "marked": "^17.0.1",
    "pdfjs-dist": "^5.4.624",
    "qr-scanner": "^1.4.2",
    "qrcode": "^1.5.4",
    "rxjs": "~7.8.2",
    "sanitize-html": "^2.17.0",
    "skia-canvas": "^3.0.8",
    "stripe": "^18.5.0",
    "superjson": "^2.2.6",
    "tslib": "^2.8.1",
    "type-fest": "^5.4.3",
    "ws": "^8.19.0",
    "zone.js": "~0.16.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.1.2",
    "@angular/compiler-cli": "^21.1.3",
    "@axe-core/playwright": "^4.11.1",
    "@ngneat/falso": "^8.0.2",
    "@playwright/test": "1.58.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/eslint-plugin-query": "^5.91.4",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/dom-speech-recognition": "^0.0.7",
    "@types/express": "^5.0.6",
    "@types/google.maps": "^3.58.1",
    "@types/html-to-text": "^9.0.4",
    "@types/jsdom": "^27.0.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.10.11",
    "@types/qrcode": "^1.5.6",
    "@types/sanitize-html": "^2.16.0",
    "@types/ws": "^8.18.1",
    "angular-eslint": "21.2.0",
    "drizzle-kit": "1.0.0-beta.14-a36c63d",
    "drizzle-seed": "1.0.0-beta.14-a36c63d",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-perfectionist": "^5.5.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "jsdom": "^27.4.0",
    "playwright-core": "^1.58.1",
    "playwright-ng-schematics": "21.0.3",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "packageManager": "bun@1.3.7"
}
