{
  "name": "evorto",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --no-hmr",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:evorto": "node dist/evorto/server/server.mjs",
    "serve:instrumented:evorto": "node --import ./instrument.mjs dist/evorto/server/server.mjs",
    "lint": "ng lint",
    "update": "ng update @angular/cli @angular/cdk @angular/core @angular/material --allow-dirty",
    "e2e": "ng e2e",
    "e2e:ui": "ng e2e --ui --workers 2",
    "generate:colors": "ng generate @angular/material:theme-color --include-high-contrast --primary-color=#0891b2 --neutral-color=#71717a --interactive=false --directory=src/ --force=true",
    "generate:colors:esn": "ng generate @angular/material:theme-color --include-high-contrast --primary-color=#00aeef --tertiary-color=#ec008c --neutral-color=#737373 --interactive=false --directory=src/_esn --force=true",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org lukas-heddendorp --project evorto ./dist && sentry-cli sourcemaps upload --org lukas-heddendorp --project evorto ./dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.3",
    "@angular/cdk": "19.1.1",
    "@angular/common": "^19.1.3",
    "@angular/compiler": "^19.1.3",
    "@angular/core": "^19.1.3",
    "@angular/forms": "^19.1.3",
    "@angular/material": "19.1.1",
    "@angular/material-luxon-adapter": "^19.1.1",
    "@angular/platform-browser": "^19.1.3",
    "@angular/platform-browser-dynamic": "^19.1.3",
    "@angular/platform-server": "^19.1.3",
    "@angular/router": "^19.1.3",
    "@angular/ssr": "^19.1.4",
    "@fortawesome/angular-fontawesome": "1.0.0",
    "@fortawesome/duotone-regular-svg-icons": "^6.7.2",
    "@neondatabase/serverless": "^0.10.4",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sentry/angular": "^8.51.0",
    "@sentry/cli": "^2.41.1",
    "@sentry/node": "^8.51.0",
    "@sentry/profiling-node": "^8.51.0",
    "@tailwindcss/typography": "0.5.16",
    "@tanstack/angular-query-experimental": "5.64.2",
    "@tinymce/tinymce-angular": "^8.0.1",
    "@trpc/client": "^11.0.0-rc.729",
    "@trpc/server": "^11.0.0-rc.729",
    "cookie-parser": "^1.4.7",
    "drizzle-orm": "^0.38.4",
    "effect": "^3.12.7",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-openid-connect": "^2.17.1",
    "luxon": "^3.5.0",
    "rxjs": "~7.8.1",
    "superjson": "^2.2.2",
    "tslib": "^2.8.1",
    "ws": "^8.18.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.4",
    "@angular/cli": "^19.1.4",
    "@angular/compiler-cli": "^19.1.3",
    "@axe-core/playwright": "^4.10.1",
    "@ngneat/falso": "^7.3.0",
    "@playwright/test": "1.50.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@tanstack/eslint-plugin-query": "^5.64.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^4.17.21",
    "@types/jasmine": "~5.1.5",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.10",
    "@types/ws": "^8.5.14",
    "angular-eslint": "19.0.2",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.2",
    "drizzle-seed": "^0.3.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-perfectionist": "^4.7.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "playwright-core": "^1.50.0",
    "playwright-ng-schematics": "1.2.2",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.19.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "8.21.0"
  },
  "packageManager": "yarn@4.5.3"
}
