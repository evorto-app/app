    "@tiptap/extension-text-style": "^3.19.0",
    "@tiptap/pm": "^3.19.0",
    "@tiptap/starter-kit": "^3.19.0",
    "@trpc/client": "^11.9.0",
    "@trpc/server": "^11.9.0",
    "auth0": "^5.3.0",
